{
  "name": "@axistaylor/cli-testing-library",
  "version": "0.1.2",
  "description": "Small but powerful library for testing CLI the way it is used by people.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "npm run watch:lib",
    "build:cleanup": "rm -rf lib && rm -rf dist",
    "build:lib": "tsc --project tsconfig.build.json",
    "build": "npm run build:cleanup && npm run build:lib",
    "build:ci": "npm run build",
    "prepare": "npm run build:lib",
    "watch:lib": "tsc --watch --project tsconfig.build.json",
    "test": "jest test"
  },
  "author": "Georgy Marchuk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/axistaylor/cli-testing-library.git"
  },
  "dependencies": {
    "keycode": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.32",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "prompts": "^2.4.2",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "yargs": "^17.7.2"
  }
}
